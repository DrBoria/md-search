{
  "name": "vscode-astx",
  "version": "0.0.0-development",
  "displayName": "Dude, Where's My Module?",
  "description": "structural search and replace for JavaScript and TypeScript",
  "publisher": "jedwards1211",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onView:astx.astxView",
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "astx.astxView",
          "name": "Astx"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint $npm_package_config_lint",
    "lint:fix": "eslint $npm_package_config_lint",
    "prettier": "prettier --write $npm_package_config_prettier",
    "prettier:check": "prettier --list-different $npm_package_config_prettier",
    "clean": "rimraf out",
    "check:types": "tsc -p src/extension/tsconfig.json --noEmit && tsc -p src/webview/tsconfig.json --noEmit",
    "build": "npm run clean && tsc -p src/extension/tsconfig.json && webpack",
    "test": "rimraf .nyc_output; node ./out/test/runTest.js && nyc report && nyc report --reporter=lcov",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov; codecov",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release",
    "build:watch": "nodemon -e js,ts,json --watch src --exec \"(pnpm build)\"",
    "pretest": "BABEL_ENV=coverage npm run build",
    "vscode:prepublish": "npm run clean && npm run prettier:check && npm run lint && npm test && npm run build"
  },
  "release": {
    "verifyConditions": [
      "semantic-release-vsce",
      "@semantic-release/github"
    ],
    "prepare": {
      "path": "semantic-release-vsce",
      "packageVsix": "vscode-astx.vsix"
    },
    "publish": [
      "semantic-release-vsce",
      {
        "path": "@semantic-release/github",
        "assets": "vscode-astx.vsix"
      }
    ]
  },
  "config": {
    "lint": "--cache --ext .js,.ts src",
    "prettier": ".babelrc.js *.{json,md} {src,test}/**/*.{js,ts}",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm run check:types",
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,css,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@jedwards1211/commitlint-config"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codemodsquad/vscode-astx.git"
  },
  "keywords": [
    "search",
    "replace",
    "refactoring",
    "structural-search",
    "semantic-search"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codemodsquad/vscode-astx/issues"
  },
  "homepage": "https://github.com/codemodsquad/vscode-astx#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jedwards1211/commitlint-config": "^1.0.2",
    "@jedwards1211/eslint-config": "^2.0.2",
    "@jedwards1211/eslint-config-typescript": "^2.0.2",
    "@types/chai": "^4.2.14",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^16.9.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/vscode": "^1.40.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@typescript-eslint/typescript-estree": "^4.32.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "codecov": "^3.8.1",
    "copy": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "semantic-release-vsce": "^5.1.1",
    "ts-loader": "^9.3.1",
    "typescript": "4.7.4",
    "vscode-test": "^1.2.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.0.1",
    "astx": "^2.0.0-beta",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
